<Window x:Class="PasswordGenerator.Other_Prompt"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PasswordGenerator"
        mc:Ignorable="d"
        Title="Other_Prompt" Height="180" Width="220">
    <Grid Margin="0,0,0.2,1.4">
        <Label Content="Enter name other prompt:"  Margin="10,10,10.4,101"></Label>
        <TextBox x:Name="textBox1" HorizontalAlignment="Left" Height="30"  PreviewTextInput="textBox1_TextChanged"
                 TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="180" Margin="16,59,0,0"/>
        <Button x:Name="button" Content="Save" HorizontalAlignment="Left" Margin="150,112,0,0" VerticalAlignment="Top" Width="46" Height="18" Click="button_Click"/>
    </Grid>
</Window>
